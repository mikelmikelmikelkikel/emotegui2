local function NCFC_fake_script() -- RigSwitcherButtons.ClickScript 
	local script = Instance.new('LocalScript', RigSwitcherButtons)

	local r6button = script.Parent.R6
	local r15button = script.Parent.R15
	local r6emoteframe = script.Parent.Parent.R6Emotes
	local r15emoteframe = script.Parent.Parent.R15Emotes
	
	
	function leftClick()
		r6emoteframe.Visible = true
		r15emoteframe.Visible = false
	end
	
	function leftClick2()
		r6emoteframe.Visible = false
		r15emoteframe.Visible = true
	end
	
	script.Parent.R6.MouseButton1Click:Connect(leftClick)
	script.Parent.R15.MouseButton1Click:Connect(leftClick2)
end
coroutine.wrap(NCFC_fake_script)()
