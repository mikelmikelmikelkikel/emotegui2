-- Scripts:

local function FQOM_fake_script() -- Minimize.MinimizeScript 
	local script = Instance.new('LocalScript', Minimize)

	function leftClick()
		script.Parent.Parent.Parent.SoundScript:WaitForChild("ClickSound"):Play()
		script.Parent.Parent.Parent.Position = UDim2.new(0.02,-0,0.966,-0)
	end 
	
	script.Parent.MouseButton1Click:Connect(leftClick)
	
end
coroutine.wrap(FQOM_fake_script)()
local function KFFS_fake_script() -- Close.CloseScript 
	local script = Instance.new('LocalScript', Close)

	function leftClick()
		script.Parent.Parent.Parent.SoundScript:WaitForChild("ShutdownSound"):Play()
		script.Parent.Parent.Parent.Parent:Destroy()
	end
	
	script.Parent.MouseButton1Click:Connect(leftClick)
	
end
coroutine.wrap(KFFS_fake_script)()
local function NEJNYGT_fake_script() -- Maximize.MaximizeScript 
	local script = Instance.new('LocalScript', Maximize)

	function leftClick()
		script.Parent.Parent.Parent.SoundScript:WaitForChild("ClickSound"):Play()
		script.Parent.Parent.Parent.Position = UDim2.new(0.01, 0,0.181, 0)
	end
	
	script.Parent.MouseButton1Click:Connect(leftClick)
	
end
coroutine.wrap(NEJNYGT_fake_script)()
local function LSHCDQ_fake_script() -- EmoteButton.EmoteScript 
	local script = Instance.new('LocalScript', EmoteButton)

	local Anim = Instance.new("Animation")
	Anim.AnimationId = "rbxassetid://6559590606"
	local track = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(Anim)
	local HeadThrowACTIVE = false
	local HeadThrow = script.Parent
	
	HeadThrow.MouseButton1Click:connect(function()
		
		HeadThrowACTIVE = not HeadThrowACTIVE
		if HeadThrowACTIVE then
			script.Parent.Image = "rbxassetid://6559255312"
			while wait() do
				if track.IsPlaying == false then
					if HeadThrowACTIVE then
						track:Play(.1, 1, 1)
					end
				end
			end
		else
			track:Stop()
			script.Parent.Image = "rbxassetid://6559255431"
		end
	end)
end
coroutine.wrap(LSHCDQ_fake_script)()
